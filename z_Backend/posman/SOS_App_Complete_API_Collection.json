      "item": [
        {
          "name": "Create SOS Case",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 201) {",
                  "    const responseJson = pm.response.json();",
                  "    if (responseJson.data && responseJson.data.case && responseJson.data.case._id) {",
                  "        pm.collectionVariables.set('caseId', responseJson.data.case._id);",
                  "        console.log('Case ID saved to collection variable');",
                  "    }",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
